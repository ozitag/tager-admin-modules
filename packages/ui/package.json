{
  "name": "@tager/admin-ui",
  "version": "1.0.15",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozitag/tager-admin-modules.git",
    "directory": "packages/ui"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/main.d.ts",
  "type": "module",
  "exports": {
    "./css": "./dist/style.css",
    ".": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "find . -name dist -o -name \"*.tsbuildinfo\" -maxdepth 1 | xargs rm -rf",
    "build:js": "vite build",
    "build:package": "yarn build:js && yarn emit-declarations",
    "emit-declarations": "tsc --project tsconfig.app.json --emitDeclarationOnly  --declarationMap false",
    "storybook": "start-storybook --port 3000",
    "tager:update": "yarn upgrade --latest --scope @tager"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^4.0.1",
    "@floating-ui/dom": "^0.4.5",
    "@tager/admin-services": "workspace:*",
    "@tager/admin-wysiwyg": "^1.0.0",
    "autosize": "^5.0.1",
    "leaflet": "^1.8.0",
    "lodash-es": "*",
    "smoothscroll-polyfill": "^0.4.4",
    "sortablejs": "^1.15.0",
    "tinycolor2": "^1.4.2",
    "vanilla-picker": "^2.12.1",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@tager/rollup-plugin-lodash": "workspace:*",
    "@types/autosize": "^4.0.1",
    "@types/leaflet": "^1.7.11",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/sortablejs": "^1.13.0",
    "vite": "*"
  },
  "peerDependencies": {
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "publishConfig": {
    "access": "public"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
