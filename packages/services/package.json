{
  "name": "@tager/admin-services",
  "version": "1.1.4",
  "description": "Tager admin services",
  "author": "Ivan Saranchonkau <ivan.saranchenkov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozitag/tager-admin-modules.git",
    "directory": "packages/services"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "find . -name dist -o -name \"*.tsbuildinfo\" -maxdepth 1 | xargs rm -rf",
    "emit-declarations": "tsc --project ./tsconfig.app.json --emitDeclarationOnly --declarationMap false",
    "build:js": "vite build",
    "build:package": "yarn run --top-level clean && yarn build:js && yarn emit-declarations",
    "prepack": "yarn build:package"
  },
  "dependencies": {
    "i18next": "^21.9.1",
    "nanoid": "^4.0.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "vite": "*"
  },
  "peerDependencies": {
    "pinia": "*",
    "vue": "*",
    "vue-router": "*"
  }
}
