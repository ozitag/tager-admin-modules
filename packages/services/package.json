{
  "name": "@tager/admin-services",
  "version": "1.1.7",
  "description": "Tager admin services",
  "author": "Ivan Saranchonkau <ivan.saranchenkov@gmail.com>",
  "license": "MIT",
  "access": "public",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozitag/tager-admin-modules.git",
    "directory": "packages/services"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "typings": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "emit-declarations": "tsc --project ./tsconfig.app.json --emitDeclarationOnly --declarationMap false",
    "build:js": "vite build",
    "build:package": "yarn run --top-level clean && yarn build:js && yarn emit-declarations",
    "prepack": "yarn run build:package && yarn patch-package",
    "postpack": "yarn restore-package"
  },
  "dependencies": {
    "i18next": "^21.9.1",
    "nanoid": "^4.0.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@tager/admin-tools": "workspace:*",
    "vite": "*"
  },
  "peerDependencies": {
    "pinia": "*",
    "vue": "*",
    "vue-router": "*"
  }
}
